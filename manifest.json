{
  "manifest_version": 3,
  "name": "Food and Wealth",
  "version": "1.0",
  "description": "Analyze your spending habits on Uber Eats and DoorDash.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "fnw.png",
      "48": "fnw.png",
      "128": "fnw.png"
    }
  },
  "icons": {
    "16": "fnw.png",
    "48": "fnw.png",
    "128": "fnw.png"
  }
}